{
    "git.ignoreMissingGitWarning": true,
    "editor.wordWrap": "on",
    "editor.minimap.maxColumn": 15,
    "editor.fontFamily": "'JetBrains Mono', 'Courier New', monospace",
    "editor.fontSize": 13,
    "editor.lineHeight": 17,
    "workbench.editor.limit.perEditorGroup": true,
    "editor.wordWrapColumn": 180,
    "workbench.editor.limit.enabled": true,
    "workbench.iconTheme": "vscode-icons",
    "editor.minimap.renderCharacters": false,
    "editor.columnSelection": false,
    "vsicons.dontShowNewVersionMessage": true,
    "editor.tabSize": 2,
    "liveshare.allowGuestTaskControl": true,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "liveshare.focusBehavior": "accept",
    "editor.formatOnSaveMode": "modifications",
    "files.trimTrailingWhitespace": true,
    "editor.minimap.scale": 3,
    "liveshare.autoShareServers": false,
    "liveshare.codeLens": false,
    "liveshare.increasedGuestLimit": false,
    "liveshare.openSharedServers": false,
    "liveshare.shareExternalFiles": false,
    "debug.showInlineBreakpointCandidates": false,
    "editor.renderWhitespace": "all",
    "editor.smoothScrolling": true,
    "files.insertFinalNewline": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "textMateRules": [
                {
                    "name": "Comments",
                    "scope": "comment",
                    "settings": {
                        "foreground": "#7ba094",
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Functions",
                    "scope": [
                        "entity.name.function",
                        "meta.selector.css entity.name.tag",
                        "entity.name.method - source.java"
                    ],
                    "settings": {
                        "foreground": "#26c190",
                        "fontStyle": "italic bold"
                    }
                },
                {
                    "name": "Css classes",
                    "scope": "meta.selector.css",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Storage",
                    "scope": "storage.type.js",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Attributes HTML",
                    "scope": "entity.other.attribute-name.html , meta.attribute",
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "text HTML",
                    "scope": "text.html.derivative",
                    "settings": {}
                }
            ]
        }
    },
    "editor.fontLigatures": true,
    "window.zoomLevel": 0,
    "liveshare.keepAliveInterval": 10,
    "liveshare.presence": false,
    "editor.linkedEditing": true,
    "liveshare.allowGuestDebugControl": true,
    "liveshare.nameTagVisibility": "Never",
    "liveshare.allowInvites": "all",
    "remote.SSH.defaultForwardedPorts": [
        22
    ],
    "remote.SSH.suppressWindowsSshWarning": true,
    "remote.SSH.enableX11Forwarding": false,
    "workbench.startupEditor": "newUntitledFile",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features",
        "files.encoding": "utf8"
    },
    "[asp]": {
        "editor.defaultFormatter": "vscode.html-language-features",
        "files.encoding": "windows1252"
    },
    "[scss,css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "files.encoding": "utf8"
    },
    "[js]": {
        "editor.defaultFormatter": "vscode.css-language-features",
        "files.encoding": "utf8"
    },
}